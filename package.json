{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "cross-env DEBUG=false NODE_OPTIONS=\"--inspect\" NEXT_DEV_NO_PROGRESS=1 next dev",
    "dev:debug": "cross-env DEBUG=true NODE_OPTIONS=\"--inspect\" NEXT_DEV_NO_PROGRESS=1 next dev",
    "dev:quiet": "cross-env LOG_LEVEL=ERROR NEXT_TELEMETRY_DISABLED=1 NEXT_SUPPRESS_LOGS=true NEXT_METRICS_DEBUG=false NEXT_DISABLE_METRICS=1 next dev",
    "dev:verbose": "cross-env NEXT_VERBOSE_LOGGING=true NEXT_TELEMETRY_DISABLED=1 next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "node server.js",
    "lint": "eslint app src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint app src --ext .js,.jsx,.ts,.tsx --fix",
    "audit:fix": "npm audit fix",
    "clean": "rimraf .next",
    "build:prod": "cross-env NODE_ENV=production npm run clean && npm run lint && next build",
    "verify": "node scripts/verify-build.js",
    "verify:seo": "node scripts/seo-verify.js",
    "verify:seo:json": "node scripts/seo-verify.js --json --output seo-report.json",
    "verify:seo:fast": "node scripts/seo-verify.js --fail-fast",
    "test": "npm run test:jest && npm run test:vitest",
    "test:jest": "jest",
    "test:vitest": "vitest run --config vitest.config.ts",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "test:env": "cross-env NODE_ENV=development node --loader ts-node/esm src/lib/config/test-env.ts"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^16.1.4",
    "@swc/core": "^1.11.10",
    "@swc/jest": "^0.2.37",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@types/aria-query": "^5.0.4",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.20.7",
    "@types/estree": "^1.0.7",
    "@types/estree-jsx": "^1.0.5",
    "@types/hast": "^3.0.4",
    "@types/jest": "^29.5.14",
    "@types/json-schema": "^7.0.15",
    "@types/lodash": "^4.17.16",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.14.1",
    "@types/node-fetch": "^2.6.12",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@types/unist": "^3.0.3",
    "@types/webpack": "^5.28.5",
    "@types/workbox-background-sync": "^4.3.2",
    "@types/workbox-cacheable-response": "^4.3.1",
    "@types/workbox-core": "^4.3.1",
    "@types/workbox-expiration": "^4.3.1",
    "@types/workbox-precaching": "^4.3.1",
    "@types/workbox-routing": "^4.3.1",
    "@types/workbox-strategies": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^16.1.4",
    "googleapis": "^171.0.0",
    "graphql-request": "^7.1.2",
    "happy-dom": "^17.4.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-fetch": "^2.7.0",
    "postcss": "^8.4.35",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^3.0.9",
    "web-vitals": "^4.2.4",
    "zod": "^3.24.2"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.68.0",
    "@types/pg": "^8.16.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "dotenv": "^16.4.7",
    "gray-matter": "^4.0.3",
    "markdown-to-jsx": "^7.7.3",
    "marked": "^15.0.7",
    "next": "^16.1.4",
    "nodemailer": "^6.10.0",
    "pg": "^8.17.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-intersection-observer": "^9.16.0",
    "react-markdown": "^9.0.3",
    "remark": "^15.0.1",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "swr": "^2.3.3",
    "tailwind-merge": "^2.6.0",
    "uuid": "^11.0.5"
  }
}
