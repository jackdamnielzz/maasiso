{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env DEBUG=false NODE_OPTIONS=\"--inspect\" NEXT_DEV_NO_PROGRESS=1 next dev",
    "dev:debug": "cross-env DEBUG=true NODE_OPTIONS=\"--inspect\" NEXT_DEV_NO_PROGRESS=1 next dev",
    "dev:quiet": "cross-env LOG_LEVEL=ERROR NEXT_TELEMETRY_DISABLED=1 NEXT_SUPPRESS_LOGS=true NEXT_METRICS_DEBUG=false NEXT_DISABLE_METRICS=1 next dev",
    "dev:verbose": "cross-env NEXT_VERBOSE_LOGGING=true NEXT_TELEMETRY_DISABLED=1 next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "node server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "audit:fix": "npm audit fix",
    "clean": "rimraf .next",
    "build:prod": "cross-env NODE_ENV=production npm run clean && next build --no-lint",
    "verify": "node scripts/verify-build.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:env": "cross-env NODE_ENV=development node --loader ts-node/esm src/lib/config/test-env.ts"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.1.6",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.19.0",
    "eslint-config-next": "^15.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-fetch": "^2.7.0",
    "postcss": "^8.5.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "web-vitals": "^4.2.4"
  },
  "dependencies": {
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "ip": "^2.0.1",
    "markdown-to-jsx": "^7.7.3",
    "next": "15.1.6",
    "react-markdown": "^9.0.3",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^2.6.0"
  }
}
